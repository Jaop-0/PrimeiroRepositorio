/*CSS RESET*/

*{ 
    border:0;
    margin:0;
    padding:0;
    }

main{
    position:relative;
    width:100%;
    height:700%;
    background-color:#836FFF;
    margin:auto;
}

#menu_superior{
    position:relative;
    width:100%;
    height:20px;
    background-color:#c5dbf2;
}

#menu_superior a{
    width:7%;
    font-size:13px;
    font-family:verdana;
    text-align:right;
    color:#000;
    text-decoration:none;
    float:left;
    padding-top:5px;
    padding-bottom:5px;
}

nav img{
    position:relative;
    left:340%; 
    float:right;
}

#figura1{
    position:relative;
    left:5%;
}

#conectar{
    position: relative;
    font-family:calibri;
    text-align:center;
    padding:13px;
    font-size:13pt;
    color:#000;
    left:37%;
    top:10px;
    margin:auto;
    padding:5px;  
    border:solid 2px #000;
    width: 80px;
    height:130px;
    background-color:#c3d9f5;
}

#cadastre{
    position: relative;
    font-family:calibri;
    text-align:center;
    padding:13px;
    font-size:10pt;
    color:#fff;
    left:38%;
    top:10px;
    margin:auto;
    padding:5px;  
    width: 150px;
    height:200px;
    background-color:#007c89;
}
  

   


 
   
    
